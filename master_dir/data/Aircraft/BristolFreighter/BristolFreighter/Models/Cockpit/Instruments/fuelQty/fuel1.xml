<?xml version="1.0"?>
<!-- fwd tank
Syd Adams
-->
<PropertyList>

    <path>fuel.ac</path>

    <animation>
        <type>material</type>
        <object-name>FuelQTY.face</object-name>
        <object-name>FuelQTY.tank</object-name>
        <object-name>FuelQTY.needle</object-name>
        <emission>
            <red>0.045</red>
            <green>0.025</green>
            <blue>0.015</blue>
            <factor-prop>systems/electrical/outputs/instrument-lights</factor-prop>
        </emission>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>FuelQTY.needle</object-name>
        <property>consumables/fuel/tank[1]/level-lbs</property>
        <interpolation>
            <entry><ind>0.0</ind><dep>0.0</dep></entry>
            <entry><ind>2452</ind><dep>320</dep></entry>
        </interpolation>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>
    
<animation>
<type>range</type>
<min-m>0</min-m>
<max-m>5</max-m>
</animation>

</PropertyList>
